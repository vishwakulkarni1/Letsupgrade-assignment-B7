   "source": [
    "x = int(input(\"Enter starting point: \"))  \n",
    "y = int(input(\"Enter end point: \"))  \n",
    "\n",
    "for i in range(x, y):\n",
    "   order = len(str(i))\n",
    "   sum = 0\n",
    "   temp = i\n",
    "   while temp > 0:\n",
    "       digit = temp % 10\n",
    "       sum += digit ** order\n",
    "       temp //= 10\n",
    "\n",
    "   if i == sum:\n",
    "       break\n",
    "print(i)"
   ]
